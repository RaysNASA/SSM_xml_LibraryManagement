<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis_norepeat.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwz.ssm.mapper.TypeMapper">

    <select id="findById" resultType="com.lwz.ssm.entity.Type">
        select * from t_type where id=#{tid}
    </select>
    <select id="findAll" resultType="com.lwz.ssm.entity.Type">
        select * from t_type
    </select>
</mapper>